<%= form_for [@category, @article] do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br>
  <%= f.label :email %><br>
  <%= f.text_field :email %><br>
  <%= f.label :content %><br>
  <%= f.text_field :content %><br>
  <%= f.label :price %><br>
  <%= f.number_field :price %><br>

  <%= f.submit %>
<% end %>
